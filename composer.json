{
	"name": "vivait/POData-DoctrineService",
	"type": "library",
	"description": "Provides a service to allow easy exposure of Doctrine entities through POData",
	"keywords": [ "rest", "odata", "doctrine", "podata" ],
	"license": "MIT",
	"authors": [
		{
			"name": "Viva IT Limited",
			"email": "lewis@vivait.co.uk"
		}
	],
	"minimum-stability": "stable",

	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/balihoo/POData"
		}
	],

	"require": {
		"balihoo/POData" : "dev-master",
		"doctrine/common": "2.4.*",
		"doctrine/dbal": "2.4.*",
		"doctrine/orm": "2.4.*",
		"doctrine/doctrine-bundle": "1.2.*",
		"php": ">=5.3.0"
	},

	"autoload": {
		"psr-0": {
			"POData": "src/",
			"UnitTests" : "tests/"
		}
	}
}